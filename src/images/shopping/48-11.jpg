        <script>
            $(function () {
                //$('#navbarAsideDropdown0').dropdown('toggle');
                $("#prothumbs").flexisel({
                    visibleItems: 6,
                    itemsToScroll: 3,
                    autoPlay: {
                        enable: false,
                        interval: 5000,
                        pauseOnHover: true
                    }
                });
                //
                $('.btn_tag').click(function () {
                    var t = $(this).data('tag');
                    $('.btn_tag').removeClass('active');
                    if (t === 'commend') {
                        $('#description').removeClass('bopen');
                        $('#description').addClass('bclose');
                        $('#commend').removeClass('bclose');
                        $('#commend').addClass('bopen');

                    } else {
                        $('#description').removeClass('bclose');
                        $('#description').addClass('bopen');
                        $('#commend').removeClass('bopen');
                        $('#commend').addClass('bclose');
                    }
                    $(this).addClass('active');
                });
                //
                function itemCountCheck(target) {
                    var cc = target.children('.list-group-item').length;
                    var tgID = target.prop('id');
                    console.log(cc);
                    if (tgID === 'listGroup1') {
                        if (cc > 5) {
                            target.next('.btn_more').show();
                            target.addClass('bclose');
                        } else {
                            target.next('.btn_more').hide();
                            target.addClass('bopen');
                        }
                    } else {
                        if (cc > 2) {
                            target.next('.btn_more').show();
                            target.addClass('bclose');
                        } else {
                            target.next('.btn_more').hide();
                            target.addClass('bopen');
                        }
                    }

                }
                itemCountCheck($('#listGroup1'));
                itemCountCheck($('#listGroup2'));

                $('.btn_more').click(function () {
                    var tg = $(this).prev('.list-group');
                    $(this).hide();
                    tg.removeClass('bclose');
                    tg.addClass('bopen');
                });
                //
                //數量更改
                //增加、減少按鈕狀態改變
                function countBtnMode(target) {
                    console.log('數量改變');

                    var txt_goodsCount = Number(target.val());
                    var btn_countUp = target.parent().children('.btn_countUp');
                    var btn_countDown = target.parent().children('.btn_countDown');
                    if (txt_goodsCount > 1) {
                        var max_count = target.parent().children('.maxCount').val();
                        btn_countDown.prop('disabled', false);
                        if (txt_goodsCount < max_count) {
                            btn_countUp.prop('disabled', false);
                        } else {
                            btn_countUp.prop('disabled', true);
                            target.val(max_count);
                        }
                    } else {
                        btn_countDown.prop('disabled', true);
                        btn_countUp.prop('disabled', false);
                    }

                }
                $('.txt_goodsCount').change(function () {
                    countBtnMode($(this));
                });
                //增加
                $('.btn_countUp').click(function (e) {
                    e.preventDefault();
                    var max_count = $(this).parent().children('.maxCount').val();
                    console.log('數量上限：' + max_count);
                    var target_count = $(this).parent().children('.txt_goodsCount');
                    if (Number(target_count.val()) < max_count) {
                        var v = Number(target_count.val()) + 1;
                        target_count.val(v);
                    }
                    countBtnMode(target_count);
                });
                //減少
                $('.btn_countDown').click(function (e) {
                    e.preventDefault();
                    var max_count = $(this).parent().children('.maxCount').val();
                    console.log('數量上限：' + max_count);
                    var target_count = $(this).parent().children('.txt_goodsCount');
                    if (Number(target_count.val()) <= max_count) {
                        var v = Number(target_count.val()) - 1;
                        target_count.val(v);
                    }
                    countBtnMode(target_count);
                });
            });
        </script>

    


